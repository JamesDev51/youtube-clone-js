{
  "name": "jamestube_project",
  "version": "1.0.0",
  "description": "cloning youtube with using javascript skills",
  "main": "init.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore 'scss'",
    "dev:assets": "cd src && cross-env  WEBPACK_ENV=development && webpack -w ",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production && webpack",
    "build:server": "babel src --out-dir build --ignore src/static --ignore src/assets --ignore src/webpack.config.js",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "tunnel": "lt --port 4000",
    "build": "npm run build:server && npm run build:assets && npm run copyAll ",
    "prebuild": "rm -rf build",
    "start": "PRODUCTION=true node build/init.js",
    "prestart": "npm run build"
  },
  "author": "James/Minseok Jeong",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "autoprefixer": "^10.2.1",
    "aws-sdk": "^2.824.0",
    "axios": "^0.21.0",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "buffer": "^6.0.3",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.1.1",
    "method-override": "^3.0.0",
    "mini-css-extract-plugin": "^1.3.3",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "mongoose": "^5.10.11",
    "mongoose-moment": "^0.1.3",
    "mongoose-timezone": "^1.3.0",
    "morgan": "^1.10.0",
